<button id="toggleChatbot"><i class="fa-solid fa-comment" style="font-size: 2rem;"></i></button> <!-- Button to toggle chatbot visibility -->

    <div class="card" id="chatbot">
        <button class="close-btn" onclick="closeChatbot()">Close</button>
        <div class="header">
            Blog Chatbot
        </div>
        <div class="message" id="bot">
            Welcome! How can I assist you with your blog-related queries?
        </div>
        <div class="input">
            <input type="text" id="userInput" placeholder="Ask a question...">
            <button onclick="handleUserInput()">Send</button>
        </div>
    </div>
    <script>
          const blogQuestions = [
            {
                question: "How do I start a blog?",
                answer: "Starting a blog involves choosing a niche, selecting a platform (like WordPress or Blogger), and creating valuable content."
            },
            {
                question: "What topics can I write about on my blog?",
                answer: "You can write about your passions, hobbies, experiences, or expertise. Consider what interests your target audience."
            },
            {
                question: "How can I drive traffic to my blog?",
                answer: "Promote your blog through social media, SEO optimization, guest posting, and engaging with other bloggers."
            },
            {
                question: "What's the ideal blog post length?",
                answer: "There's no one-size-fits-all answer, but aim for 1,000 to 2,000 words for in-depth content."
            },
            {
                question: "How often should I publish new blog posts?",
                answer: "Consistency matters. Publish at a frequency that you can sustain—whether it's weekly, biweekly, or monthly."
            },
            {
                question: "How do I monetize my blog?",
                answer: "Explore options like affiliate marketing, sponsored posts, selling digital products, or offering premium content."
            }
        ];

        function getBotResponse(userInput) {
            const cleanedInput = userInput.toLowerCase();
            const matchedQuestion = blogQuestions.find(item => cleanedInput.includes(item.question.toLowerCase()));
            if (matchedQuestion) {
                return matchedQuestion.answer;
            } else {
                return "I'm sorry, I don't have an answer for that question.";
            }
        }

        function handleUserInput() {
            const userInput = document.getElementById("userInput").value;
            const botResponse = getBotResponse(userInput);
            displayMessage(userInput, "user");
            displayMessage(botResponse, "bot");
            document.getElementById("userInput").value = ""; // Clear the input field after sending the message
        }

        function displayMessage(message, sender) {
            const chatDiv = document.createElement("div");
            chatDiv.className = "message";
            if (sender === "bot") {
                chatDiv.style.backgroundColor = "#f9f9f9";
            }
            chatDiv.textContent = message;
            document.getElementById("bot").parentNode.insertBefore(chatDiv, document.getElementById("bot").nextSibling);
        }



        const chatbot = document.getElementById("chatbot");
        const toggleChatbotBtn = document.getElementById("toggleChatbot");

        // Function to toggle chatbot visibility
        toggleChatbotBtn.addEventListener("click", () => {
            chatbot.classList.toggle("show");
        });

        // Function to close the chatbot
        function closeChatbot() {
            chatbot.classList.remove("show");
        }

    </script>
